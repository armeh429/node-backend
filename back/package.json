{
	"name": "snape",
	"version": "0.58.5",
	"private": true,
	"description": "Snape project for back",
	"author": "The Regiti Team",
	"scripts": {
		"dev": "nodemon",
		"build": "rimraf ./dist && tsc && tsc-alias && npm run copyfiles",
		"start": "set NODE_ENV=production && export NODE_ENV=production && node ./dist/index.js",
		"lint": "eslint --ext .js,.jsx,.ts,.tsx .",
		"format": "prettier --ignore-path .prettierignore --config .prettierrc --write '**/*.{js,jsx,ts,tsx}'",
		"copyfiles": "npm run resources:data && npm run resources:locales",
		"resources:data": "copyfiles './resources/data/bulk/**/*.json' './dist'",
		"resources:locales": "copyfiles './resources/locales/**/*.json' './dist'",
		"regiti": "node regiti/index"
	},
	"dependencies": {
		"body-parser": "^1.20.2",
		"compression": "^1.7.4",
		"cookie-parser": "^1.4.6",
		"cors": "^2.8.5",
		"device-detector-js": "^3.0.3",
		"dotenv": "^16.4.5",
		"express": "^4.18.3",
		"express-rate-limit": "^7.2.0",
		"geoip-country": "^4.2.79",
		"helmet": "^7.1.0",
		"i18next": "^23.10.1",
		"i18next-fs-backend": "^2.3.1",
		"i18next-http-middleware": "^3.5.0",
		"jsonwebtoken": "^9.0.2",
		"mariadb": "^3.2.3",
		"moment": "^2.30.1",
		"mongoose": "^8.2.2",
		"multer": "^1.4.5-lts.1",
		"mv": "^2.1.1",
		"semver": "^7.6.0",
		"validator": "^13.11.0",
		"winston": "^3.12.0",
		"winston-loki": "^6.0.8",
		"xregexp": "^5.1.1"
	},
	"devDependencies": {
		"@types/compression": "^1.7.5",
		"@types/cookie-parser": "^1.4.7",
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.21",
		"@types/geoip-country": "^4.0.2",
		"@types/gradient-string": "^1.1.5",
		"@types/jsonwebtoken": "^9.0.6",
		"@types/mongoose": "^5.11.97",
		"@types/multer": "^1.4.11",
		"@types/mv": "^2.1.4",
		"@types/node": "^20.11.28",
		"@types/validator": "^13.11.9",
		"@typescript-eslint/eslint-plugin": "^7.2.0",
		"@typescript-eslint/parser": "^7.2.0",
		"copyfiles": "^2.4.1",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-import-resolver-typescript": "^3.6.1",
		"gradient-string": "^2.0.2",
		"nodemon": "^3.1.0",
		"prettier": "^3.2.5",
		"rimraf": "^5.0.5",
		"ts-node": "^10.9.2",
		"tsc-alias": "^1.8.8",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.4.2"
	}
}
